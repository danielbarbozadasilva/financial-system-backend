      "post": {
        "summary": "Logar usuário",
        "description": "Essa rota é responsável por autenticar o usuário",
        "tags": [
          "Autenticar Usuário"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Auth"
              },
              "examples": {
                "auth": {
                  "value": {
                    "cpf": "413.423.614-41",
                    "password": "daniel"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "500": {
            "description": "Erro ao realizar a operação!"
          },
          "403": {
            "description": "Sua conta foi desativada pelo Administrador!"
          },
          "401": {
            "description": "Cpf ou senha inválidos!\n\nNão foi possivel criar a credencial!"
          },
          "200": {
            "description": "Usuário autenticado com sucesso!"
          }
        }
      }
    },
