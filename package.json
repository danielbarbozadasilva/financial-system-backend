    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "sequelize": "^6.6.2",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2",
